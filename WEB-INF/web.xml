<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="3.0">

  <display-name>EngineGo!</display-name>


  <description>
     EngineGo is a user-based and item-based Movie Recommendation System!
  </description>

    <servlet> 
        <servlet-name>Login</servlet-name> 

        <servlet-class>Login</servlet-class> 

    </servlet>
    <servlet> 
        <servlet-name>InsertWaitinglist</servlet-name> 

        <servlet-class>InsertWaitinglist</servlet-class> 

    </servlet>
    
    <servlet> 
        <servlet-name>Signup</servlet-name> 

        <servlet-class>Signup</servlet-class> 

    </servlet>
    <servlet> 
        <servlet-name>Rate</servlet-name> 

        <servlet-class>Rate</servlet-class> 

    </servlet>
    <servlet> 
        <servlet-name>RatedList</servlet-name> 

        <servlet-class>RatedList</servlet-class> 

    </servlet>
    <servlet> 
        <servlet-name>WaitingList</servlet-name> 

        <servlet-class>WaitingList</servlet-class> 

    </servlet>

    <servlet> 
        <servlet-name>Recommend</servlet-name> 

        <servlet-class>Recommend</servlet-class> 

    </servlet>
    <servlet> 
        <servlet-name>News</servlet-name> 

        <servlet-class>News</servlet-class> 

    </servlet>
 

    <servlet-mapping>   

        <servlet-name>Login</servlet-name>

        <url-pattern>/login</url-pattern>  

     </servlet-mapping>
     <servlet-mapping>   

        <servlet-name>Signup</servlet-name>

        <url-pattern>/signup</url-pattern>  

     </servlet-mapping>
     <servlet-mapping>   

        <servlet-name>Rate</servlet-name>

        <url-pattern>/rate</url-pattern>  

     </servlet-mapping>
     <servlet-mapping>   

        <servlet-name>RatedList</servlet-name>

        <url-pattern>/ratedlist</url-pattern>  

     </servlet-mapping>
          <servlet-mapping>   

        <servlet-name>WaitingList</servlet-name>

        <url-pattern>/waitinglist</url-pattern>  

     </servlet-mapping>
     
     <servlet-mapping>   

        <servlet-name>Recommend</servlet-name>

        <url-pattern>/recommend</url-pattern>  

     </servlet-mapping>
     
     <servlet-mapping>   

        <servlet-name>News</servlet-name>

        <url-pattern>/news</url-pattern>  

     </servlet-mapping>
     <servlet-mapping>   

        <servlet-name>InsertWaitinglist</servlet-name>

        <url-pattern>/insertwaitinglist</url-pattern>  

     </servlet-mapping>

 

</web-app>